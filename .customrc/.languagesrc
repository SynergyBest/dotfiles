if [[ -d $HOME/.langs ]]; then
    if [[ -d "$HOME/.langs/Python/3.9.4" ]]; then
        PATH="$HOME/.langs/Python/3.9.4/bin:$PATH"
    fi
    if [[ -d "$HOME/.langs/go" ]]; then
        PATH="$HOME/.langs/go/bin:$PATH"
    fi
    if [[ -d "$NVMDIR" ]]; then
        [ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"
    fi
    if [[ -d "/usr/local/opt/ruby" ]]; then
        PATH="/usr/local/opt/ruby/bin:$PATH"
        PATH="/usr/local/lib/ruby/gems/3.0.0/bin:$PATH"
    fi
fi